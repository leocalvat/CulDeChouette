<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.culdechouette">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CulDeChouette">
        <activity
            android:name=".StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SiroterActivity"
            android:label="@string/sirotage" />
        <activity
            android:name=".ChouetteVeluteActivity"
            android:label="@string/chouette_velute" />
        <activity
            android:name=".SuiteActivity"
            android:label="@string/suite" />
        <activity
            android:name=".SoufletteActivity"
            android:label="@string/souflette" />
        <activity
            android:name=".GrelottineActivity"
            android:label="@string/grelottine" />
        <activity
            android:name=".SummaryActivity"
            android:label="@string/summary"/>
    </application>

</manifest>